# Hardware service port
PORT=3002

# Use real hardware (not mock)
MOCK_MODE=false

# Serial port for relay board
USB_TTL_PORT=COM3

# Logging level
LOG_LEVEL=info

# CORS allowed origins (add your production URLs)
ALLOWED_ORIGINS=http://localhost:8080,http://localhost:5173
