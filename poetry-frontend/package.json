{
  "name": "poetry-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node ../tools/logs/frontend/dev-with-log.mjs",
    "dev:raw": "vite",
    "dev:log": "node ../tools/logs/frontend/dev-with-log.mjs",
    "predev": "npm run i18n:gen && npm run i18n:verify",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "i18n:gen": "node ../tools/ci/i18n/i18n-generate-keys.mjs",
    "i18n:verify": "node ../tools/ci/i18n/i18n-verify.mjs",
    "sdk:generate": "openapi-ts -i ../docs/api/backend-generated/v1/openapi.yaml -o src/api/generated -c @hey-api/client-fetch -l ../logs/frontend && node tools/sdk/add-ts-nocheck-to-generated.mjs",
    "typecheck": "npm run i18n:gen && npm run i18n:verify && tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "typecheck:only": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "build": "npm run i18n:gen && npm run i18n:verify && npm run format && npm run lint -- --max-warnings=0 && npm run typecheck && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "npm run i18n:gen && npm run i18n:verify && vitest run",
    "test:watch": "npm run i18n:gen && npm run i18n:verify && vitest",
    "pw:test": "playwright test",
    "pw:test:ui": "playwright test --ui",
    "pw:codegen": "playwright codegen http://localhost:5173",
    "pw:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.87.1",
    "eslint-plugin-react": "^7.37.5",
    "jwt-decode": "^4.0.0",
    "poetry-repo-tools": "file:..",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@hey-api/openapi-ts": "^0.84.4",
    "@playwright/test": "^1.48.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.4"
  }
}
